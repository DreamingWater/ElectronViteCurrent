{
  "name": "LaserController",
  "version": "2.3.2",
  "main": "dist-electron/main/index.js",
  "description": "Laser Controller",
  "author": "",
  "license": "MIT",
  "private": true,
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ],
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:5173/"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && electron-builder",
    "build-only": "vue-tsc --noEmit && vite build && electron-builder",
    "build:renderer": "esbuild ./src/preload/render.js --outfile=./dist-electron/preload/render.js --platform=browser --format=esm",
    "build:serialControl": "esbuild src/preload/serialControl.ts --outfile=./dist-electron/preload/serialControl.js --platform=browser --format=esm",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@antv/g2": "^5.0.7",
    "@antv/g2plot": "^2.3.5",
    "@vitejs/plugin-vue": "^4.1.0",
    "chart.js": "^4.3.3",
    "child_process": "^1.0.2",
    "core-js": "^3.8.3",
    "cross-spawn": "^7.0.3",
    "echarts": "^5.4.3",
    "electron": "^31.3.1",
    "electron-builder": "^24.4.0",
    "electron-rebuild": "^3.2.9",
    "element-plus": "^2.3.8",
    "esbuild": "^0.20.2",
    "normalize.css": "^8.0.1",
    "pinia": "^2.0.34",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "style-loader": "^3.3.1",
    "typescript": "^5.0.2",
    "vite": "^4.1.4",
    "vite-plugin-electron": "^0.12.0",
    "vite-plugin-electron-renderer": "^0.14.1",
    "vue": "^3.2.47",
    "vue-router": "^4.0.3",
    "vue-tsc": "^1.2.0"
  },
  "dependencies": {
    "@tomjs/electron-devtools-installer": "^2.3.2",
    "electron-window-state": "^5.0.3",
    "serialport": "^12.0.0",
    "sweetalert2": "^11.12.2"
  }
}
